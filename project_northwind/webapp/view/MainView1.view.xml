<mvc:View controllerName="projectnorthwind.controller.MainView1"
	height="100%"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
    displayBlock="true">

       <Page
		id="page"
        title="{i18n>title}"
		showHeader="true" >
		<customHeader>
			<Bar id="_IDGenBar1">
				<contentRight>
					<Button id="edit" text="Edit" enabled="false" press="handleEditPress" />
					<Button id="save" text="Save" type="Emphasized" visible="false" press="handleSavePress" />
					<Button id="cancel" text="Cancel" visible="false" press="handleCancelPress" />
				</contentRight>
			</Bar>
		
		</customHeader>
		
	</Page>
  
</mvc:View>
